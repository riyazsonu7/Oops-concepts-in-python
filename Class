class Student:
    def __init__(self):
        self.name='riyaz'
        self.age=21
        self.marks=85
        #print('__init__ is called')
    def talk(self):
        print("name - ",self.name)
        print('age - ',self.age)
        print('marks - ',self.marks)



s1=Student()
